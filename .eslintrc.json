{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": [
      1,
      {
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ],
    "no-duplicate-imports": [
      "warn",
      {
        "includeExports": false
      }
    ],
    "no-await-in-loop": ["warn"]
  }
}
